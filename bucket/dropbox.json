            "dropbox.exe",
            "Dropbox"
        ]
    ],
    "checkver": {
        "url": "https://www.dropboxforum.com/t5/user/viewprofilepage/user-id/308787",
        "regex": "(?sm)Dropbox-desktop-client-builds/Stable-Build-(\\d+)-(\\d+)-(\\d+)",
        "replace": "${1}.${2}.${3}"
    },
    "autoupdate": {
